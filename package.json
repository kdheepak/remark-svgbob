{
        "name": "remark-svgbob",
        "version": "1.1.0",
        "description": "A remark plugin to convert a markdown codeblock with ascii art to svg",
        "scripts": {
                "build": "tsup src/index.ts --format esm --dts --clean",
                "dev": "vitest",
                "test": "vitest run"
        },
        "author": "Dheepak Krishnamurthy",
        "license": "MIT",
        "devDependencies": {
                "remark": "^15.0.1",
                "ts-node": "^10.9.2",
                "tsup": "^8.0.2",
                "typescript": "^5.4.2",
                "vite": "^5.1.5",
                "vitest": "^1.3.1"
        },
        "type": "module",
        "main": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "files": [
                "dist"
        ],
        "exports": {
                ".": "./dist/index.js",
                "./package.json": "./package.json"
        },
        "keywords": [
                "remark-plugin",
                "remark",
                "svgbob"
        ],
        "dependencies": {
                "unist-util-visit": "^5.0.0"
        }
}
