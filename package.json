{
  "name": "remark-svgbob",
  "version": "1.1.1",
  "description": "A remark plugin to convert a markdown codeblock with ascii art to svg",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "vitest",
    "test": "vitest run"
  },
  "author": "Dheepak Krishnamurthy",
  "license": "MIT",
  "devDependencies": {
    "hast-util-from-html": "^2.0.0",
    "mdast-util-mdx-jsx": "^3.1.0",
    "remark": "^15.0.1",
    "remark-mdx": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.5",
    "vitest": "^1.3.1"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "keywords": [
    "remark-plugin",
    "remark",
    "svgbob"
  ],
  "dependencies": {
    "hast-util-to-html": "^9.0.4",
    "unist-util-visit": "^5.0.0"
